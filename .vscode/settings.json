{
	"editor.autoClosingDelete": "always",
	"editor.autoClosingQuotes": "always",
	"editor.defaultFormatter": "JohnnyMorganz.stylua3",
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.snippetSuggestions": "top",
	"editor.suggest.showWords": false,
	"gitlens.codeLens.enabled": false,
	"json.format.enable": false,
	"robloxLsp.completion.endAutocompletion": true,
	"robloxLsp.diagnostics.disable": ["undefined-doc-name", "undefined-type"],
	"robloxLsp.diagnostics.globals": ["PluginManager"],
	"robloxLsp.hint.paramName": true,
	"robloxLsp.hint.returnType": true,
	"robloxLsp.hint.setType": true,
	"robloxLsp.misc.color3Picker": false,
	"robloxLsp.misc.serverPort": 5467,
	"robloxLsp.typeChecking.mode": "Non Strict",
	"robloxLsp.typeChecking.options": {"union-bivariance": false},
	"robloxLsp.workspace.ignoreDir": [],
	"robloxLsp.workspace.loadMode": "Rojo Project",
	"robloxLsp.workspace.maxPreload": 100,
	"robloxLsp.workspace.preloadFileSize": 85,
	"robloxLsp.workspace.rojoProjectFile": "default",
	"rojo.buildFileFormat": "Binary",
	"rojo.releaseBranch": "7.x",
	"selene.run": "onSave",
	"stylua.styluaPath": "C:/Path/stylua2Roact.exe",
	"stylua.styluaRoactPath": "C:/Path/stylua2Roact.exe",
	"terminal.integrated.defaultProfile.windows": "Command Prompt",
}